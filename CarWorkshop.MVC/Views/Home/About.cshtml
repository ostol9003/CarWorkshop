@model About

<h1>@Model.Title</h1>
<div class="alert alert-primary">@Model.Description</div>

<p>Tags:</p>
<ul>
    @foreach (var tag in Model.Tags)
    {
        <li>@tag</li>
    }
</ul>

<div class="container text-center">
    <div class="row">
        <div class="col-sm-4 border">
            Column
        </div>
        <div class="col-sm-1 border">
            Column
        </div>
        <div class="col-sm-1 border">
            Column
        </div>
        <div class="col-sm-6 border">
            Column
        </div>
    </div>
</div>